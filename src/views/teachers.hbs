<div class="container mt-5">
  <h1>Teacher List</Table></h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      {{#each teachers}}
      <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td><a href="students/{{this.id}}">View Your Students</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>